<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="faq">
	
	
	<insert id="faqWriteEnd">
		insert into faq values
		( seq_faq_no.nextval, #{question}, #{answer}, #{type})
	
	</insert>
	<resultMap type="faq" id="faqMap">
	</resultMap>
	 <select id="faqTicketList" resultMap="faqMap">
		select *
		from faq
		<where>
			<if test="type != '' and type != null">
				type = #{type}			
			</if>
						
		</where>
	</select>
	 <select id="faqBuyList" resultMap="faqMap">
		select *
		from faq
		where type='B'
	</select>
	 <select id="faqMemberList" resultMap="faqMap">
		select *
		from faq
		where type='M'
	</select>
	 <select id="faqCouponList" resultMap="faqMap">
		select *
		from faq
		where type='C'
	</select>
	 <select id="faqElseList" resultMap="faqMap">
		select *
		from faq
		where type='E'
	</select>
	 <select id="faqAllList" resultMap="faqMap">
		select *
		from faq
	</select>
	
		
</mapper>